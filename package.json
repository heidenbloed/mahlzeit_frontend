{
  "name": "recipe-db-frontend",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build-no-check": "DEBUG=vite-plugin-pwa:* vite build",
    "serve": "vite preview --port 4000",
    "start": "npm run build-no-check && npm run serve",
    "https-serve": "PORT=5000 serve dist",
    "https-start": "npm run build-no-check && npm run https-serve"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "vue": "^3.2.16",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.9.3",
    "autoprefixer": "^10.4.0",
    "https-localhost": "^4.7.0",
    "postcss": "^8.4.4",
    "tailwindcss": "^3.0.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vite-plugin-pwa": "^0.11.10",
    "vue-tsc": "^0.29.8",
    "workbox-core": "^6.4.0",
    "workbox-precaching": "^6.4.0",
    "workbox-routing": "^6.4.0",
    "workbox-window": "^6.4.0"
  }
}
